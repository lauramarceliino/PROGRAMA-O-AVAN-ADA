1
#include <stdio.h>

int main()
{
    int i=3,j=5;
    int *p, *q;
    p = &i;
    q = &j;
     printf( "1. p == &i;: %d\n", p == &i); 
     printf( "2. *p - *q;: %d\n", *p - *q);
     printf( "3. **&p;: %d\n", **&p);
     printf( "4. 3 - *p/(*q) + 7;: %d\n", 3 - *p/(*q) + 7);

    return 0;
}


2.
#include <stdio.h>

int main() {
    int i=5, *p;
    p = &i;
    printf("%x %d %d %d %d", p,*p+2,**&p,3**p,**&p+4);

    return 0;
}
